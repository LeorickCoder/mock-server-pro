{
  "name": "mock-server-pro",
  "version": "1.1.0",
  "description": "A powerful mock server for frontend development with path parameter support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "docs"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "ts-node src/dev-server.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "prepare": "npm run build"
  },
  "keywords": [
    "mock",
    "server",
    "development",
    "testing",
    "api",
    "express",
    "middleware",
    "frontend"
  ],
  "author": "leorick <leorickjameson@gmail.com> (https://leorick.com)",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "glob": "^10.3.10",
    "chokidar": "^3.5.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.19",
    "@types/cors": "^2.8.17",
    "@types/body-parser": "^1.19.5",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  }
} 